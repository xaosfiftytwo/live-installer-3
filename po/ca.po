msgstr "Actualitzant la memòria cau de l'APT  "

#: ../usr/lib/live-installer-3/installer.py:678
msgid "Update the new system"
msgstr "Actualitza el nou sistema "

#: ../usr/lib/live-installer-3/installer.py:685
msgid "Localizing packages"
msgstr "Configurant llengua dels paquets "

#: ../usr/lib/live-installer-3/installer.py:703
msgid "Setting keyboard options"
msgstr "Establint les opcions del teclat "

#: ../usr/lib/live-installer-3/installer.py:765
msgid "Configuring sensors"
msgstr "Configurant els sensors "

#: ../usr/lib/live-installer-3/installer.py:771
msgid "Removing VirtualBox"
msgstr "Eliminant VirtualBox "

#: ../usr/lib/live-installer-3/installer.py:777
msgid "Removing os-prober"
msgstr "Eliminant OS-prober "

#: ../usr/lib/live-installer-3/installer.py:783
msgid "Installing bootloader"
msgstr "Instal·lant el carregador d'arrencada "

#: ../usr/lib/live-installer-3/installer.py:805
msgid ""
"The grub bootloader was not configured properly! You need to configure it "
"manually."
msgstr ""
"El gestor d'arrencada GRUB no està configurat correctament! Cal configurar-"
"lo manualment."

#: ../usr/lib/live-installer-3/installer.py:808
msgid "Grub not configured"
msgstr "El GRUB no està configurat "

#: ../usr/lib/live-installer-3/installer.py:834
msgid "Cleaning APT"
msgstr "Netejant l'APT "

#: ../usr/lib/live-installer-3/installer.py:866
msgid "Removing live configuration (packages)"
msgstr "S'està eliminant la configuració live (paquets) "

#: ../usr/lib/live-installer-3/installer.py:908
msgid "Unmounting partitions"
msgstr "Desmuntant les particions "

#: ../usr/lib/live-installer-3/installer.py:930
