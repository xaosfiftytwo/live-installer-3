msgstr "国"

#: ../usr/lib/live-installer-3/gtk_interface.py:544
msgid "Selected timezone:"
msgstr "タイムゾーンの選択"

#: ../usr/lib/live-installer-3/gtk_interface.py:545
msgid "Select timezone"
msgstr "タイムゾーンの選択"

#: ../usr/lib/live-installer-3/gtk_interface.py:548
msgid "Refresh"
msgstr "再読込"

#: ../usr/lib/live-installer-3/gtk_interface.py:549
