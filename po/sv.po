#: ../usr/lib/live-installer-3/gtk_interface.py:523
msgid "User info"
msgstr "Användarinformation"

#: ../usr/lib/live-installer-3/gtk_interface.py:524
msgid "Partitioning"
msgstr "Partitionering"

#: ../usr/lib/live-installer-3/gtk_interface.py:525
msgid "Please make sure you wish to manage partitions manually"
msgstr "Var vänlig och se till så att du önskar hantera partitioner manullet"

#: ../usr/lib/live-installer-3/gtk_interface.py:526
msgid "Summary"
msgstr "Sammanfattning"

#: ../usr/lib/live-installer-3/gtk_interface.py:527
msgid "Installing {}..."
msgstr "Installerar {} ..."

#: ../usr/lib/live-installer-3/gtk_interface.py:528
msgid "Installation is paused: please finish the custom installation"
msgstr ""
"Installationen är hejdad: var vänlig och slutför den anpassade installation"

#: ../usr/lib/live-installer-3/gtk_interface.py:535
#: ../usr/lib/live-installer-3/gtk_interface.py:565
